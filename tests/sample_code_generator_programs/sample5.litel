make x assign 0;

check (x greater_than 5)
{
    shout("x is: ");
    shout(x);
    x assign x add 1;
}

check (x less_than 5)
{
    shout("x is: ");
    shout(x);
    make y assign subtract 2;
    check (y less_than 0)
    {
        shout("y is : ");
        shout(y);
        y assign y add 1;
    }
    x assign x add 1;
}